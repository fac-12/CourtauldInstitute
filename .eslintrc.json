{
  "extends": ["airbnb", "prettier"],
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "always"],
    "no-console": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-indent": ["error", 2],
    "react/prefer-stateless-function": [0],
    "react/prop-types": 0,
    "import/prefer-default-export": 0
  }
}
